
- 회원인증테이블에 암호화 알고리즘을 저장한다.

ALTER TABLE auth.tb_auth_member ADD CRYPTOGRAPHIC_ALGORITHM varchar(10) DEFAULT 'AES256' COMMENT '암호화 알고리즘' NULL;
ALTER TABLE auth.tb_auth_member CHANGE CRYPTOGRAPHIC_ALGORITHM CRYPTOGRAPHIC_ALGORITHM varchar(10) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT 'AES256' NULL COMMENT '암호화 알고리즘' AFTER PASSWORD;

