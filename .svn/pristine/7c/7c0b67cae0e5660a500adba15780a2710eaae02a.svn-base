package org.snubi.auth.packet.enums;

public enum EnumHttpResponse {
	SUCESS	("200"),
	ERROR	("400");
	
	private final String text;    
	EnumHttpResponse(final String text) {
	      this.text = text;
	}    
	@Override
	public String toString() {
		return text;
	}
}

